<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="minScaleFactor" format="float"/>
    <attr name="maxScaleFactor" format="float"/>
    <attr name="autoScaleTime"  format="integer"/>
    <declare-styleable name="GalleryView">
        <attr name="minScaleFactor" />
        <attr name="maxScaleFactor" />
